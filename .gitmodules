[submodule "libraries/curl"]
	path = libraries/curl
	url = https://github.com/curl/curl
[submodule "libraries/zlib"]
	path = libraries/zlib
	url = https://github.com/madler/zlib.git
	ignore = dirty
[submodule "libraries/libpng"]
	path = libraries/libpng
	url = https://github.com/glennrp/libpng.git
[submodule "libraries/fltk"]
	path = libraries/fltk
	url = https://github.com/fltk/fltk
[submodule "libraries/protobuf"]
	path = libraries/protobuf
	url = https://github.com/protocolbuffers/protobuf.git
	branch = 3.5.x
[submodule "libraries/miniupnp"]
	path = libraries/miniupnp
	url = https://github.com/miniupnp/miniupnp.git
[submodule "libraries/jsoncpp"]
	path = libraries/jsoncpp
	url = https://github.com/open-source-parsers/jsoncpp.git
	branch = 0.y.z
	ignore = dirty
[submodule "libraries/portmidi"]
	path = libraries/portmidi
	url = https://github.com/PortMidi/portmidi.git
